#!/usr/bin/env python

from gi.repository import Gtk

class gBsdConv(object):       
	def __init__(self):
	    builder = Gtk.Builder()
	    builder.add_from_file("gbsdconv.xml")
	    self.window = builder.get_object("window1")
	    self.window.show()

if __name__ == "__main__":
	app = gBsdConv()
	Gtk.main()
